[Unit]
Description=${project.description}-${version}
After=syslog.target

[Service]
User=root
SuccessExitStatus=143
ExecStart=/bin/bash -c "/usr/bin/java -jar /var/${project.name}/${ service.profile == 'test' ? '' : version + "/" }${project.name}.jar"
WorkingDirectory=/var/${project.name}/${ service.profile == 'test' ? '' : version + "/" }

[Install]
WantedBy=multi-user.target